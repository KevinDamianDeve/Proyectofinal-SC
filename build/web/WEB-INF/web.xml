<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegistrarPagoSecre</servlet-name>
        <servlet-class>Controlador.RegistrarPagoSecre</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recuperard</servlet-name>
        <servlet-class>Controlador.recuperard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recuperare</servlet-name>
        <servlet-class>Controlador.recuperare</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SERVLOGINPADRE</servlet-name>
        <servlet-class>Controlador.SERVLOGINPADRE</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletmodificaReunion</servlet-name>
        <servlet-class>Controlador.ServletmodificaReunion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrarPagoSecre</servlet-name>
        <url-pattern>/RegistrarPagoSecre</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>recuperard</servlet-name>
        <url-pattern>/recuperard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>recuperare</servlet-name>
        <url-pattern>/recuperare</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SERVLOGINPADRE</servlet-name>
        <url-pattern>/SERVLOGINPADRE</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletmodificaReunion</servlet-name>
        <url-pattern>/ServletmodificaReunion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
